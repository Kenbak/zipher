{
  "name": "ziphers-wallet",
  "version": "0.1.0",
  "description": "Privacy-first Zcash wallet browser extension by CipherScan",
  "type": "module",
  "scripts": {
    "dev": "parcel watch src/popup/index.html src/background/service-worker.ts --dist-dir dist --no-hmr",
    "build": "parcel build src/popup/index.html src/background/service-worker.ts --dist-dir dist && node scripts/post-build.cjs",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@chainsafe/webzjs-keys": "file:../WebZjs/packages/webzjs-keys",
    "@chainsafe/webzjs-wallet": "file:../WebZjs/packages/webzjs-wallet",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.16.1",
    "@parcel/optimizer-htmlnano": "^2.16.1",
    "@parcel/transformer-postcss": "^2.16.1",
    "@parcel/transformer-typescript-tsc": "^2.16.1",
    "@types/bip39": "^3.0.0",
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.11.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.16.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenbak/zcash-wallet-extension.git"
  },
  "keywords": [
    "zcash",
    "wallet",
    "chrome-extension",
    "privacy",
    "cryptocurrency",
    "webzjs"
  ],
  "author": "CipherScan Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenbak/zcash-wallet-extension/issues"
  },
  "homepage": "https://github.com/kenbak/zcash-wallet-extension#readme"
}
